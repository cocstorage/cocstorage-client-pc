{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@api": [
        "./src/api"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@dto": [
        "./src/dto"
      ],
      "@dto/*": [
        "./src/dto/*"
      ],
      "@library": [
        "./src/library"
      ],
      "@library/*": [
        "./src/library/*"
      ],
      "@styles": [
        "./src/styles"
      ],
      "@styles/*": [
        "./src/styles/*"
      ]
    },
    "module": "esnext",
    "target": "es5",
    "sourceMap": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
